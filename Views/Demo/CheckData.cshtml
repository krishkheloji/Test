@model Mixed

<form method="post">
    Username <input type="text" asp-for="log.Username" class="form-control" /><br />

    Password <input type="password" asp-for="log.Password" class="form-control" /><br />
    <input type="submit" value="Login" class="btn btn-primary"/>
</form>

@if(Model.log!=null)
{
    @Model.log.Username <br>
    @Model.log.Password
}

<table class="table">
    <thead>
        <tr>
            <th scope="col">Username</th>
            <th scope="col">Password</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var e in Model.logs)
        {
            <tr>
                <th scope="row">@e.Username</th>
                <td>@e.Password</td>
            </tr>
        }
    </tbody>
</table>
